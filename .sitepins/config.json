{
  "content": "content",
  "media": "assets/images",
  "public": "static",
  "code": "layouts",
  "configs": [
    "config",
    "data"
  ],
  "custom-commit": false,
  "arrangement": [
    {
      "type": "file",
      "targetPath": "content/_index.md",
      "groupName": "Homepage"
    },
    {
      "type": "folder",
      "targetPath": "content",
      "groupName": "Pages",
      "include": "*.md",
      "exclude": "_index.md"
    },
    {
      "type": "folder",
      "targetPath": "content/blog",
      "groupName": "Blog",
      "include": "*.md",
      "exclude": "_index.md"
    },
    {
      "type": "folder",
      "targetPath": "content/events",
      "groupName": "Event Pages",
      "include": "*.md",
      "exclude": ""
    }
  ]
}